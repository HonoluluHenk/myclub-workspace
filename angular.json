{
  "version": 1,
  "projects": {
    "myclub-shared": {
      "projectType": "library",
      "root": "libs/myclub-shared",
      "sourceRoot": "libs/myclub-shared/src",
      "prefix": "myclub",
      "architect": {
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": [
              "libs/myclub-shared/tsconfig.lib.json",
              "libs/myclub-shared/tsconfig.spec.json"
            ],
            "exclude": ["**/node_modules/**", "!libs/myclub-shared/**"]
          }
        },
        "test": {
          "builder": "@nrwl/jest:jest",
          "options": {
            "jestConfig": "libs/myclub-shared/jest.config.js",
            "tsConfig": "libs/myclub-shared/tsconfig.spec.json",
            "setupFile": "libs/myclub-shared/src/test-setup.ts"
          }
        }
      },
      "schematics": {
        "@nrwl/angular:component": {
          "styleext": "scss"
        }
      }
    },
    "scraper-poc": {
      "root": "apps/scraper-poc",
      "sourceRoot": "apps/scraper-poc/src",
      "projectType": "application",
      "prefix": "scraper-poc",
      "schematics": {},
      "architect": {
        "build": {
          "builder": "@nrwl/node:build",
          "options": {
            "outputPath": "dist/apps/scraper-poc",
            "main": "apps/scraper-poc/src/main.ts",
            "tsConfig": "apps/scraper-poc/tsconfig.app.json",
            "assets": ["apps/scraper-poc/src/assets"]
          },
          "configurations": {
            "production": {
              "optimization": true,
              "extractLicenses": true,
              "inspect": false,
              "fileReplacements": [
                {
                  "replace": "apps/scraper-poc/src/environments/environment.ts",
                  "with": "apps/scraper-poc/src/environments/environment.prod.ts"
                }
              ]
            }
          }
        },
        "serve": {
          "builder": "@nrwl/node:execute",
          "options": {
            "buildTarget": "scraper-poc:build"
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": [
              "apps/scraper-poc/tsconfig.app.json",
              "apps/scraper-poc/tsconfig.spec.json"
            ],
            "exclude": ["**/node_modules/**", "!apps/scraper-poc/**"]
          }
        },
        "test": {
          "builder": "@nrwl/jest:jest",
          "options": {
            "jestConfig": "apps/scraper-poc/jest.config.js",
            "tsConfig": "apps/scraper-poc/tsconfig.spec.json"
          }
        }
      }
    }
  },
  "cli": {
    "defaultCollection": "@nrwl/angular"
  },
  "schematics": {
    "@nrwl/angular:application": {
      "unitTestRunner": "jest",
      "e2eTestRunner": "cypress"
    },
    "@nrwl/angular:library": {
      "unitTestRunner": "jest"
    },
    "@schematics/angular:component": {
      "changeDetection": "OnPush"
    }
  },
  "defaultProject": "scraper-poc"
}
